<?xml version="1.0"?>
<Container version="2">
  <Name>caddy-security-cfdns</Name>
  <Repository>warfair1337/caddy-security-cfdns</Repository>
  <Registry>https://ghcr.io/warfair1337/caddy-security-cfdns:latest</Registry>
  <Network>br0</Network>
  <MyIP>1.2.3.4</MyIP>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/warfair1337/caddy-security-cfdns</Project>
